{
  "all": {
    "sourceCodeDir": "app",
    "additionalEntrypoints": [
      "~/pages/*.tsx",
      "~/emails/*.tsx",
      "~modules/@fontsource-variable/bricolage-grotesque/files/bricolage-grotesque-latin-wght-normal.woff2",
      "~modules/@fontsource-variable/manrope/files/manrope-latin-wght-normal.woff2"
    ],
    "ssrBuildEnabled": true,
    "ssrEntrypoint": "~/ssr/inertia.tsx",
    "ssrOutputDir": "dist/ssr"
  },
  "development": {
    "publicOutputDir": "dist-dev",
    "port": 3036,
    "autoBuild": true,
    "skipProxy": false
  },
  "test": {
    "publicOutputDir": "dist-test",
    "port": 3037
  },
  "production": {
    "publicOutputDir": "dist"
  }
}
