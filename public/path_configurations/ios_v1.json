{
  "settings": {},
  "rules": [
    {
      "patterns": [".*"],
      "properties": {
        "context": "default",
        "pull_to_refresh_enabled": true
      }
    },
    {
      "patterns": ["^/login$"],
      "properties": {
        "context": "modal",
        "pull_to_refresh_enabled": false
      }
    },
    {
      "patterns": ["/new$"],
      "properties": {
        "context": "modal",
        "modal_style": "medium",
        "pull_to_refresh_enabled": false
      }
    }
  ]
}
